  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "clean": "rm -rf dist/* && rm -rf es/*",
    "compile": "rm -rf es/* && tsc",
    "test": "NODE_ENV=test jest --coverage",
    "test:ci": "NODE_ENV=test jest --silent --ci --coverage --colors",
    "lint": "tslint -t stylish -c ./tslint.json \"src/**/*.{ts,tsx}\"",
    "lint:fix": "tslint -t stylish --fix -c ./tslint.json \"src/**/*.{ts,tsx}\"",
    "lint:css": "stylelint \"src/**/*.{ts,tsx}\"",
    "lint:sass": "sass-lint --verbose",
    "lint:sass:fix": "sass-lint-auto-fix -c .sass-lint.yml --verbose",
    "build": "npm run clean && webpack",
    "build:watch": "webpack --watch",
    "styleguide:build": "styleguidist build",
    "styleguide": "styleguidist server",
    "gh-pages": "git subtree push --prefix styleguide origin gh-pages",
    "codequality": "docker run --env CODECLIMATE_FORMAT=html --env SOURCE_CODE=$(pwd) --volume $(pwd):/code --volume /var/run/docker.sock:/var/run/docker.sock quay.io/intellihr/codequality /code",
    "rebuild": "rm -Rf public/build node_modules public/js/bundle.js && yarn && yarn run build"
  },
